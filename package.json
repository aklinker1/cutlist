{
  "name": "onshape-cutlist",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/aklinker1/onshape-cutlist"
  },
  "keywords": [
    "onshape",
    "cutlist",
    "woodworking"
  ],
  "license": "MIT",
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1+npm@gmail.com"
  },
  "files": [
    "bin",
    "dist/npm"
  ],
  "bin": {
    "cutlist": "./bin/cutlist.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/npm/index.d.ts",
        "import": "./dist/npm/index.js"
      }
    }
  },
  "module": "./dist/npm/index.js",
  "types": "./dist/npm/index.d.ts",
  "scripts": {
    "dev": "bun run --watch src/cli.ts",
    "build:npm": "bun build.npm.ts",
    "build:bin": "bun build src/cli.ts --compile --outfile dist/cutlist",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "base64-js": "^1.5.1",
    "c12": "^1.10.0",
    "consola": "^3.2.3",
    "ofetch": "^1.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aklinker1/check": "^1.1.0",
    "@types/bun": "latest",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "publint": "^0.2.7",
    "simple-git-hooks": "^2.11.0",
    "typescript": "^5.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "changelog": {
    "excludeAuthors": [
      "aaronklinker1@gmail.com"
    ]
  }
}
